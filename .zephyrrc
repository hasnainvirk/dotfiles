# toolchain setup
export ZEPHYR_TOOLCHAIN_VARIANT=zephyr
export BOARD=nrf9160dk_nrf9160ns

# west aliases
alias build-native='west build -p auto -b native_posix'
alias build-arm='west build -p auto -b qemu_cortex_m3'
alias build='west build -p auto'
alias run='west build -t run'
alias flash='west flash'
alias menuconfig='west build -t menuconfig'
